#, python-format
msgid "Could not open %s"
msgstr "Impossible d'ouvrir %s"

#: umc/python/quota/user.py:140
#, python-format
msgid "Could not remove the following user: %s"
msgstr "Impossible de supprimer l’utilisateur suivant : %s"

#: umc/python/quota/tools.py:181
#, python-format
msgid "Device %r could not be found"
msgstr "Le périphérique %r est introuvable"

