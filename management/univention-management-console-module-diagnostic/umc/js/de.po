msgid "Problem repaired: "
msgstr "Probleme repariert: "

#: umc/js/diagnostic.js:67
msgid "Problem: "
msgstr "Problem: "

#: umc/js/diagnostic.js:207
msgid "Run system diagnosis"
msgstr "System-Diagnose starten"

#: umc/js/diagnostic.js:286
msgid "Running full diagnosis..."
msgstr "Komplette Diagnose läuft..."

#: umc/js/diagnostic.js:66
msgid "Success: "
msgstr "Erfolg: "

#: umc/js/diagnostic.js:64
msgid "Warning: "
msgstr "Warnung: "

